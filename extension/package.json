{
  "name": "bugstalker",
  "displayName": "BugStalker",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Debuggers"
  ],
  "activationEvents": [
    "onDebug"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "bugstalker.helloWorld",
        "title": "Hello World"
      }
    ],
    "breakpoints": [
      {
        "language": "rust"
      }
    ],
    "debuggers": [
      {
        "type": "bugstalker",
        "label": "BugStalker",
        "languages": [
          "rust"
        ],
        "configurationAttributes": {
          "launch": {
            "required": [
              "program"
            ],
            "properties": {
              "program": {
                "type": "string",
                "description": "Path to the program to debug."
              },
              "args": {
                "type": [
                  "array"
                ],
                "items": {
                  "type": "string"
                },
                "description": "Program arguments.",
                "default": []
              },
              "cwd": {
                "type": "string"
              },
              "debugMode": {
                "type": "bool"
              }
            }
          }
        },
        "initialConfigurations": [
          {
            "type": "bugstalker",
            "request": "launch",
            "name": "BugStalker"
          }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.98.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "eslint": "^9.21.0",
    "typescript": "^5.7.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
