{
  "name": "bugstalker",
  "displayName": "BugStalker",
  "description": "Debugging Rust applications with BugStalker in Visual Studio Code",
  "license": "MIT",
  "version": "0.1.1",
  "engines": {
    "vscode": "^1.98.0"
  },
  "platforms": [
    "linux"
  ],
  "architecture": {
    "type": [
      "x64"
    ]
  },
  "keywords": [
    "rust",
    "debug"
  ],
  "author": {
    "name": "Konstantin Derevtsov",
    "email": "godzie@yandex.ru"
  },
  "contributors": [
    {
      "name": "Hasan Mohsin",
      "email": "hasan@hasali.dev"
    }
  ],
  "categories": [
    "Debuggers"
  ],
  "activationEvents": [
    "onDebug"
  ],
  "repository": {
    "url": "https://github.com/godzie44/BugStalker"
  },
  "bugs": {
    "url": "https://github.com/godzie44/BugStalker/issues"
  },
  "main": "./out/extension.js",
  "publisher": "BugStalker",
  "icon": "icon.png",
  "contributes": {
    "commands": [
      {
        "command": "bugstalker.helloWorld",
        "title": "Hello World"
      }
    ],
    "breakpoints": [
      {
        "language": "rust"
      }
    ],
    "debuggers": [
      {
        "type": "bugstalker",
        "label": "BugStalker",
        "languages": [
          "rust"
        ],
        "configurationAttributes": {
          "launch": {
            "required": [
              "program"
            ],
            "properties": {
              "program": {
                "type": "string",
                "description": "Path to the program to debug."
              },
              "args": {
                "type": [
                  "array"
                ],
                "items": {
                  "type": "string"
                },
                "description": "Program arguments.",
                "default": []
              },
              "env": {
                "type": [
                  "array"
                ],
                "items": {
                  "type": "string"
                },
                "description": "Program environment variables.",
                "default": []
              },
              "cwd": {
                "type": "string"
              },
              "debugMode": {
                "type": "bool"
              }
            }
          }
        },
        "initialConfigurations": [
          {
            "type": "bugstalker",
            "request": "launch",
            "name": "BugStalker"
          }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.98.0",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.21.0",
    "typescript": "^5.8.3"
  }
}
